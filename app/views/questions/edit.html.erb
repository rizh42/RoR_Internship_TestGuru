<h1>Edit "<%= @question.title %>" Question</h1>

<%= form_with model: @question, local: true do |form| %>
  <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">

  <p>
    <%= form.label :question%>
    <%= form.text_field :title%>
  </p>

  <p><%= form.submit %></p>
<% end %>

<%= link_to "Back", @question.test%>